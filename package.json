{
  "name": "verrazzano",
  "version": "2.0.0",
  "description": "Streaming geospatial format conversion",
  "main": "dist/index.js",
  "keywords": [
    "gdal",
    "format",
    "to json",
    "to geojson",
    "from geojson",
    "shp",
    "kml",
    "kmz",
    "gpx",
    "gdb",
    "geojson",
    "stream",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staeco/verrazzano.git"
  },
  "contributors": [
    "Contra <yo@contra.io> (http://contra.io)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/staeco/verrazzano/issues"
  },
  "homepage": "https://github.com/staeco/verrazzano#readme",
  "files": [
    "dist"
  ],
  "engines": { "node" : ">= 12" },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "skip-full": true,
    "check-coverage": false
  },
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test": "NODE_ENV=testing nyc mocha --require @babel/register --recursive --reporter spec --bail --exit --timeout 36000 && npm run-script lint"
  },
  "resolutions": {
    "end-of-stream": "github:isaacs/end-of-stream#custom-stream-premature-close-fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@stae/babel-node": "^0.1.0",
    "@stae/linters": "^0.1.2",
    "babel-plugin-istanbul": "^5.2.0",
    "body-parser": "^1.18.3",
    "eslint": "^6.0.0",
    "eslint-cli": "^1.0.0",
    "express": "^4.16.3",
    "get-port": "^5.0.0",
    "get-stream": "^5.1.0",
    "graceful-fs": "^4.1.15",
    "into-stream": "^5.1.0",
    "mocha": "^6.0.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "should": "^13.0.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "core-js": "3.0.0",
    "duplexify": "^4.0.0",
    "end-of-stream": "github:isaacs/end-of-stream#custom-stream-premature-close-fix",
    "findit2": "^2.2.3",
    "from2": "^2.3.0",
    "gdal-next": "^0.10.0",
    "is-iso-date": "^0.0.1",
    "kml-stream": "^1.1.1",
    "lodash.mapvalues": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "merge2": "^1.2.3",
    "mkdirp": "^0.5.1",
    "pumpify": "^2.0.0",
    "tempfile": "^3.0.0",
    "through2": "^3.0.1",
    "wkx": "^0.4.7"
  }
}
